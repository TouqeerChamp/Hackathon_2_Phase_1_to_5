# Environment variables for Todo App Backend
# Copy this file to .env and fill in your values

# Neon DB PostgreSQL connection string
# Format: postgresql://user:password@ep-xyz.region.neon.tech/dbname?sslmode=require
DATABASE_URL=postgresql://user:password@ep-xyz.region.neon.tech/dbname?sslmode=require

# JWT Secret Key (minimum 32 characters for HS256)
# Generate with: python -c "import secrets; print(secrets.token_hex(32))"
JWT_SECRET=your-32-char-minimum-secret-key-here

# JWT Algorithm (default: HS256)
JWT_ALGORITHM=HS256

# API Version Prefix
API_V1_PREFIX=/api/v1
